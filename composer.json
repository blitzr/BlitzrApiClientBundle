{
    "name" : "blitzr/api-client-bundle",
    "description" : "The official Blitzr API Client for Symfony",
    "type" : "symfony-bundle",
    "authors" : [{
        "name" : "Blitzr",
        "email" : "contact@blitzr.com"
    }],
    "keywords" : [
        "blitzr api bundle"
    ],
    "license" : [
        "MIT"
    ],
    "minimum-stability": "dev",
    "repositories" : [
        {
            "type": "git",
            "url": "git@github.com:blitzr/blitzr-php-client.git"
        }
    ],
    "require" : {
        "blitzr/php-client": "dev-master"
    },
    "autoload" : {
        "psr-0" : {
            "Blitzr\\ApiClientBundle" : ""
        }
    },
    "target-dir" : "Blitzr/ApiClientBundle"
}